{
  "ci": {
    "collect": {
      "url": ["https://urbangents2.yeetonline.com/"],
      "numberOfRuns": 1
    },
    "assert": {
      "assertions": {
        "categories:performance": ["error", { "minScore": 0.6}],
        "categories:accessibility": ["error", { "minScore": 0.6 }],
        "categories:seo": ["error", { "minScore": 0.6 }]
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
